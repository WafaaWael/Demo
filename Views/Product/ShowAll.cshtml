

<table class="table table-bordered">
    @foreach(Product item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Price</td>
            <td><img src ="/Images/@item.Image" style="width:100px"/></td>
            <td><a href="/Product/deatails/@item.ID">deatails</a></td>
        </tr>
    }
   
</table>